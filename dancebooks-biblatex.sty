%\addbibresource{#1/journal-of-musicology.bib}%
\addbibresource{#1/polish.bib}%
\addbibresource{#1/portuguese.bib}%
\addbibresource{#1/problems.bib}%
\addbibresource{#1/rothenfelser.bib}%
